  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build"
