      "eslint --fix"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@loadable/component": "^5.12.0",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.4.1",
    "flickity": "^2.2.1",
    "focus-visible": "^5.1.0",
    "gatsby": "^2.20.36",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-loadable-components-ssr": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.8",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sass": "^2.2.4",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-contentful": "^2.2.10",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-sharp": "^2.4.7",
    "intersection-observer": "^0.10.0",
    "node-sass": "^4.14.0",
    "plyr-react": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flickity-component": "^3.5.0",
    "react-helmet": "^6.0.0",
    "react-masonry-layout": "^0.4.0",
    "react-pswp": "^0.0.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1"
  }
